<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="csedu.englishlearningapp">

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="AA_DB_NAME"
            android:value="Pickrand" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />
        <meta-data
            android:name="AA_MODELS"
            android:value="csedu.englishlearningapp.Models.OddManWords , csedu.englishlearningapp.Models.MatchFriendsWords,              csedu.englishlearningapp.Models.RightManEnterWords, csedu.englishlearningapp.Models.Words " />

        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.WordsActivity" />
        <activity android:name=".Activities.GamesActivity" />
        <activity android:name=".Activities.NoviceWordActivity" />
        <activity android:name=".Activities.StandardWordActivity" />
        <activity android:name=".Activities.AdvancedWordActivity" />
        <activity android:name=".Activities.OddManOutActivity" />
        <activity android:name=".Activities.MatchFriendsActivity" />
        <activity android:name=".Activities.RightManEnterActivity"></activity>
    </application>

</manifest>